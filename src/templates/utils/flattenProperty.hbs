{{~#if flattenFunction~}}
resourcedata.SetMapInterfaceArrayWithFuncIfNotNil({{object}}Map, "{{snake name}}", {{object}}.{{pascal name}}, {{flattenFunction}})
{{~else if (eq type "unknown")~}}
// TODO: Handle {{snake name}} property
{{~else if isReference~}}
resourcedata.SetMapReferenceValueIfNotNil({{object}}Map, "{{snake name}}_id", {{object}}.{{pascal name}})
{{~else if isStringArray~}}
resourcedata.SetMapStringArrayValueIfNotNil({{object}}Map, "{{snake name}}", {{object}}.{{pascal name}})
{{~else~}}
resourcedata.SetMapValueIfNotNil({{object}}Map, "{{snake name}}", {{object}}.{{pascal name}})
{{~/if~}}

