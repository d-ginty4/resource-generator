{
  "name": "callableTimeSet",
  "packageName": "outboundCallabletimeset",
  "properties": [
    {
      "name": "name",
      "type": "string",
      "description": "The name of the CallableTimeSet.",
      "required": true,
      "isStringArray": false,
      "tfSchemaData": {
        "type": "schema.TypeString"
      }
    },
    {
      "name": "callableTimes",
      "type": "array",
      "description": "The list of CallableTimes for which it is acceptable to place outbound calls.",
      "required": true,
      "isStringArray": false,
      "flattenFunction": "flattencallableTimes",
      "buildFunction": "buildcallableTimes",
      "tfSchemaData": {
        "type": "schema.TypeList",
        "element": "callableTimeResource"
      },
      "nestedObject": {
        "name": "callableTime",
        "properties": [
          {
            "name": "timeSlots",
            "type": "array",
            "description": "The time intervals for which it is acceptable to place outbound calls.",
            "required": true,
            "isStringArray": false,
            "flattenFunction": "flattencampaignTimeSlots",
            "buildFunction": "buildcampaignTimeSlots",
            "tfSchemaData": {
              "type": "schema.TypeList",
              "element": "campaignTimeSlotResource"
            },
            "nestedObject": {
              "name": "campaignTimeSlot",
              "properties": [
                {
                  "name": "startTime",
                  "type": "string",
                  "description": "The start time of the interval as an ISO-8601 string, i.e. HH:mm:ss",
                  "required": true,
                  "isStringArray": false,
                  "tfSchemaData": {
                    "type": "schema.TypeString"
                  }
                },
                {
                  "name": "stopTime",
                  "type": "string",
                  "description": "The end time of the interval as an ISO-8601 string, i.e. HH:mm:ss",
                  "required": true,
                  "isStringArray": false,
                  "tfSchemaData": {
                    "type": "schema.TypeString"
                  }
                },
                {
                  "name": "day",
                  "type": "integer",
                  "description": "The day of the interval. Valid values: [1-7], representing Monday through Sunday",
                  "required": true,
                  "isStringArray": false,
                  "tfSchemaData": {
                    "type": "schema.TypeInt"
                  }
                }
              ]
            }
          },
          {
            "name": "timeZoneId",
            "type": "string",
            "description": "The time zone for the time slots; for example, Africa/Abidjan",
            "required": true,
            "isStringArray": false,
            "tfSchemaData": {
              "type": "schema.TypeString"
            }
          }
        ]
      }
    }
  ]
}
